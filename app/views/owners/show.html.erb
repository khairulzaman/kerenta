<div class="page-header">
  <h1>Owners Management
    <small>Manage owners here</small>
  </h1>
</div>
<div class="row">
  <div class="span14">
    <h2>Details for <%= @owner.name %></h2>
    <div class="clearfix">
      <label>Name</label>
      <div>
        <%= @owner.name %>
      </div>
    </div>
    <div class="clearfix">
      <label>Telephone</label>
      <div>
        <%= @owner.telephone %>
      </div>
    </div>
    <div class="clearfix">
      <label>Address</label>
      <div>
        <%= @owner.address %>
      </div>
    </div>
  </div>
</div>
