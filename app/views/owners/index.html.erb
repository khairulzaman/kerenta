<div class="page-header">
  <h1>Owners Management
    <small>Manage owners here</small>
  </h1>
</div>
<div class="row">
  <div class="span10">
    <table>
      <thead>
        <tr>
          <th>Name</th>
          <th>Telephone</th>
        </tr>
      </thead>
      <tbody>
        <% @owners.each do |owner| %>
          <tr>
            <td>
              <%= link_to owner.name, owner %>
            </td>
            <td>
              <%= owner.telephone %>
            </td>
          </tr>
        <% end %>
        </tbody>
      </table>
  </div>
  <div class="span4">
    <%= link_to "New Owner", new_owner_path, :class => "btn primary" %>
  </div>
</div>
